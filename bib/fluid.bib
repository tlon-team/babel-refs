	langid = {english},
	timestamp = {2025-04-21 13:24:20 (GMT)},
	title = {From {AI} to distant probes},
	url = {https://magnusvinding.com/2024/03/21/from-ai-to-distant-probes/},
	urldate = {2025-04-21}
}

@online{Vinding2024ThoughtsAiPause,
	file = {~/My Drive/library-html/Vinding2024ThoughtsAiPause.html;~/My Drive/library-pdf/Vinding2024ThoughtsAiPause.pdf},
	author = {Vinding, Magnus},
	abstract = {This post discusses the arguments for and against advocating for a pause in artificial intelligence development.  Proponents of a pause emphasize the need for AI safety and governance to catch up with the rapid advancements in AI capabilities. However, the tractability and potential benefits of a pause are uncertain, especially compared to alternative efforts such as focusing on worst-case AI safety or mitigating s-risks (risks of astronomical suffering).  A common flaw in the AI pause discourse is its simplistic "doom vs. non-doom" framing, which overlooks the potential for severe suffering in human-controlled futures.  From a suffering-focused perspective, it is unclear whether a human-controlled future with advanced AI is preferable to a future where control is lost.  While ensuring the avoidance of catastrophic AI-controlled outcomes is a moral imperative, equal consideration should be given to the potential for catastrophic human-controlled outcomes. Therefore, it is not evident that advocating for an AI pause is the most effective approach to reducing future suffering. – AI-generated abstract.},
	date = {2024-06-06},
	journaltitle = {Magnus Vinding},
	langid = {english},
	timestamp = {2025-04-21 13:24:21 (GMT)},
	title = {Thoughts on {AI} pause},
	url = {https://magnusvinding.com/2024/06/06/thoughts-on-ai-pause/},
	urldate = {2025-04-21}
}

@online{Vinding2025ReducingExtremeSuffering,
	file = {~/My Drive/library-pdf/Vinding2025ReducingExtremeSuffering.pdf},
	date = {2025},
	abstract = {Extreme suffering poses unique challenges for those attempting to reduce it. These challenges include the difficulty of confronting the reality of suffering, determining effective interventions, maintaining motivation, and coping with the vast uncertainty inherent in such endeavors.  Focusing on actionable steps, such as building supportive communities and developing personal virtues like compassion, can increase effectiveness and mitigate feelings of helplessness.  Reframing the negative goal of suffering reduction into positive, constructive goals, such as accumulating resources and knowledge, offers a more sustainable motivational framework.  Addressing empathic distress and Weltschmerz through compassion-based motivation and self-care enables individuals to remain engaged without succumbing to burnout.  Cultivating realistic hope, accepting uncertainty, and connecting with others dedicated to the cause foster resilience and enhance long-term commitment.  Prioritizing self-care, overcoming guilt associated with rest and personal well-being, and reframing compassionate impact as a gradual process help maintain mental health and maximize potential for positive change. – AI-generated abstract.},
	author = {Vinding, Magnus},
	langid = {english},
	timestamp = {2025-04-21 13:23:02 (GMT)},
	title = {Reducing extreme suffering in healthy ways},
	url = {https://docs.google.com/document/d/1tU8B0BqJnjKi12XNwUD3hAkLe4HPKrXwzCixFXr5058/edit?usp=embed_facebook},
	urldate = {2025-04-21}
}

@InCollection{Vinding2025ReducingExtremeSufferingb,
	file = {~/My Drive/library-pdf/Vinding2025ReducingExtremeSufferingb.pdf},
	publisher = {unpublished},
	abstract = {Alleviating extreme suffering presents unique difficulties. These include confronting the disturbing reality of suffering's prevalence and severity, determining effective interventions, and maintaining motivation despite potential inconvenience and unpopularity.  Focusing on actionable changes, rather than intractable suffering, is recommended, emphasizing potential global impact even through small, local efforts.  Prioritizing positive, constructive goals, such as building supportive communities and generating relevant knowledge, offers a more sustainable motivational framework.  Compassionate impact should be viewed as a gradual, long-term process, prioritizing skill development and resilience.  Accepting inherent uncertainties while focusing on established best practices and personal virtues, such as compassion, honesty, and intellectual humility, is crucial.  Self-kindness and flexible optimization strategies, aiming for a "golden range" rather than a singular optimal path, help navigate uncertainties.  Empathic distress and Weltschmerz, while potentially debilitating, can be mitigated by compassion-based motivation and a focus on achievable positive change.  Connecting with like-minded individuals provides support and shared purpose. Prioritizing self-care, overcoming guilt associated with rest and personal wellbeing, and cultivating realistic hope for impactful change are essential for long-term effectiveness. – AI-generated abstract.},
	url = {https://docs.google.com/document/d/1tU8B0BqJnjKi12XNwUD3hAkLe4HPKrXwzCixFXr5058/edit?tab=t.0},
	date = {2025},
	booktitle = {Compassionate Purpose: Personal Inspiration for a Better World},
	title = {Reducing extreme suffering in healthy ways},
	author = {Vinding, Magnus},
	timestamp = {2025-06-20 11:44:52 (GMT)}
}

@online{Watson2018AssessingGlobalCatastrophic,
	file = {~/My Drive/library-pdf/Watson2018AssessingGlobalCatastrophic.pdf;~/My Drive/library-html/Watson2018AssessingGlobalCatastrophic.html},
	author = {Watson, Crystal},
	abstract = {This talk discusses global catastrophic biological risks (GCBRs), defined as biological events causing extraordinary widespread disaster beyond governmental and private sector management capabilities.  The 1918 influenza pandemic, which caused 50 to 100 million deaths, serves as a benchmark for GCBR severity.  Potential GCBR scenarios include biowarfare, bioterrorism, accidental or intentional release of engineered pathogens, ecosystem disruption eliminating food sources, and natural emergence of highly virulent and transmissible pathogens.  The eradication of smallpox, led by Dr. D.A. Henderson, demonstrates humanity's potential to combat biological threats through leadership, ingenuity, and simple tools like the bifurcated needle.  The Center for Health Security is working to define GCBRs, improve risk communication, analyze pandemic pathogen characteristics, and enhance detection capabilities.  They are also researching technologies for pandemic prevention and response, and conducting exercises like Clade X, which simulated a catastrophic pandemic causing 150 million deaths and highlighted the need for rapid vaccine development, response planning, and global cooperation.  Potential ways for individuals to contribute to GCBR mitigation include research, policy change, and raising awareness. – AI-generated abstract.},
	date = {2018-07-17},
	journaltitle = {EffectiveAltruism.org},
	langid = {english},
	timestamp = {2025-04-21 13:30:36 (GMT)},
	title = {Assessing Global Catastrophic Biological Risks {\textbar} Effective Altruism},
