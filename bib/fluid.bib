@article{Lonnroth2009DriversOfTuberculosis,
	journal = {Social Science \& Medicine},
	journaltitle = {Social Science \& Medicine},
	langid = {english},
	author = {Lönnroth, Knut and Jaramillo, Ernesto and Williams, Brian G.
                  and Dye, Christopher and Raviglione, Mario},
	title = {Drivers of tuberculosis epidemics: {The} role of risk factors
                  and social determinants},
	volume = {68},
